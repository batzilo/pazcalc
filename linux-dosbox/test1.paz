PROGRAM bar ( ) {
    int i, a = 42;
    int x[16];

    WRITELN("start");

    FOR (i, 0 TO 15) {
        WRITE("opa! ");
        if ( i >= 7 ) WRITE("- ");
        else WRITE("  ");
        WRITESP(i);
        x[i] = i;
        if ( i >= 5 ) WRITESP(i);
        if ( i > 10 ) {
            WRITESP(i);
            WRITE("ntei!");
        }
        WRITELN();
    }

    WRITELN("end");

    return;
}
