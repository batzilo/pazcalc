const int N = 3;

PROGRAM bar() {
    int i,j;
    int a[N][N];
    FOR (i, 0 TO N-1) {
        FOR(j, 0 TO N-1) {
            a[i][j] = i*N+j;
            WRITESP(a[i][j]);
        }
        WRITELN();
    }
    WRITELN();
    int k = N*N-1;
    FOR (i, 0 TO k) {
        WRITESP(i);
        WRITESP(':');
        WRITESP(a[0][i]);
        WRITELN();
        //if ( i % N == 0 ) WRITELN();
    }
    return;
}
