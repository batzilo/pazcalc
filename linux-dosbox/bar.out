xseg    segment public 'code'
    assume  cs:xseg, ds:xseg, ss:xseg
    org 100h
main    proc    near
    call    near ptr _bar_1
    mov ax,4C00h
    int 21h
main    endp
@1:
_bar_1  proc near
    push bp
    mov bp, sp
    sub sp, 0
@2:
    mov ax, 0
    push ax
@3:
    sub sp, 2
    push word ptr [bp + 4]
    call near ptr _writeInteger
    add sp, 3
@4:
    mov al, 10
    sub sp, 1
    mov si, bp
    mov byte ptr [si], al
@5:
    sub sp, 2
    push word ptr [bp + 4]
    call near ptr _writeChar
    add sp, 3
@6:
    jmp @bar_1
@7:
@bar_1: mov sp, bp
    pop bp
    ret
_bar_1  endp

extrn _writeInteger : proc
extrn _writeChar : proc

xseg    ends
    end main
