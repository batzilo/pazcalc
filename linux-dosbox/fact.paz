FUNC int fact1(int x) {
    //if (x <= 0) return -1;
    if (x == 1) return 1;
    else return x * fact1(x-1);
}

FUNC int fact2(int x) {
    if (x <= 0) return -1;
    if (x == 1) return 1;
    else return x * fact2(x-1);
}

