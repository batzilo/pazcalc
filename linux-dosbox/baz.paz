const int N = 3;

PROGRAM bar() {
    int i,j;
    int a[N][N];
    FOR(i, 0 TO N*N-1) {
        a[i][0] = i;
    }
    FOR(i, 0 TO N*N-1) {
        WRITESP(a[i][0]);
    }
    WRITELN();
    FOR(i, 0 TO N-1) {
        FOR(j, 0 TO N-1) {
            WRITESP(a[i*N][j]);
        }
    }
    WRITELN();
    return;
}
