FUNC int foo ( int a ) {
    return a*a - a;
}

FUNC int bar ( int x, int y ) {
    return x*x - 2*x*y + y*y;
}

PROGRAM baz ( ) {
    int a,b,c = 0;
    bool d;
    if ( d ) {
        while ( a < b ) {
            c += foo(c);
        }
    }
    else
        do
            c += bar(a, b);
        while ( c < 100 );
    return;
}
