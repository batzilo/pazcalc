const int N = 5;

PROGRAM bar() {
    int i,j;
    int a[N][N];
    FOR (i, 0 TO N-1) {
        FOR (j, 0 TO N-1) {
            a[i][j] = i*N+j;
            WRITESP(a[i][j]);
        }
        WRITELN();
    }
    WRITELN();
    FOR (i, 0 TO N-1) {
        FOR (j, 0 TO N-1) {
            WRITESP(a[i][j]);
        }
        WRITELN();
    }
    return;
}
